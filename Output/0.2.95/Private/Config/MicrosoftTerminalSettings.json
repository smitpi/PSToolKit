{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+alt+d"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": "resetFontSize",
            "keys": "ctrl+0"
        },
        {
            "command": {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command": {
                "action": "adjustFontSize",
                "delta": -1
            },
            "keys": "ctrl+minus"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": true
            },
            "keys": "ctrl+c"
        },
        {
            "command": "closePane",
            "keys": "ctrl+alt+w"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": {
                "action": "adjustFontSize",
                "delta": 1
            }
        }
    ],
