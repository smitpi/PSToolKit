                "audible",
                "window",
                "taskbar"
            ],
            "closeOnExit": "always",
            "colorScheme": "Argonaut",
            "cursorShape": "filledBox",
            "experimental.retroTerminalEffect": false,
            "font": 
            {
                "face": "CaskaydiaCove Nerd Font",
                "size": 13,
                "weight": "thin"
            },
            "historySize": 9001,
            "intenseTextStyle": "all",
            "tabTitle": null,
            "useAcrylic": true
        },
        "list": 
