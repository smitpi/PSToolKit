{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": true
            },
            "keys": "ctrl+c"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "adjustFontSize",
                "delta": -1
            },
            "keys": "ctrl+minus"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+alt+d"
        },
        {
            "command": 
            {
                "action": "adjustFontSize",
                "delta": 1
            }
        },
        {
            "command": "closePane",
            "keys": "ctrl+alt+w"
        },
        {
            "command": "resetFontSize",
            "keys": "ctrl+0"
        }
    ],
    "confirmCloseAllTabs": false,
    "copyFormatting": "all",
    "copyOnSelect": true,
    "defaultProfile": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
    "multiLinePasteWarning": false,
    "profiles": 
    {
        "defaults": 
        {
            "antialiasingMode": "cleartype",
            "backgroundImage": null,
            "backgroundImageOpacity": 0.0,
            "bellStyle": 
